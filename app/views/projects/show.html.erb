<div>
      <h1><%= @project.name %></h1>
      <div> <%= image_tag @project.file_url (:large) %> </div>
			<div class="like"> 51 </div>			<div class="didit"> 33 </div>
			<p><%= simple_format(@project.description) %></p>
      <p>Starts on: <%= @project.start_at.to_time.strftime('%A, %d %b %Y %l:%M %p') %></p>
      <h2>Ends on: <%= @project.end_at.to_time.strftime('%A, %d %b %Y %l:%M %p') %></h2>
      <h1><%= @project.name %></h1>
    	<% @project.uploads.each do |upload| %>
    		<inline><%= image_tag upload.file_url (:medium) %></inline>
    	<% end %>


</div>
<p><%= link_to "Add Your Photo to this Diversion", new_project_upload_path(@project) %></p>
<p><%= link_to "Edit this Diversion", edit_project_path(@project) %></p>
<p><%= link_to "Delete this Diversion", @project, method: :delete, data: {confirm: "Are you sure you want to delete this project?"}  %></p>
<p><%= link_to "Back to Daily Diversions", projects_path %></p>

